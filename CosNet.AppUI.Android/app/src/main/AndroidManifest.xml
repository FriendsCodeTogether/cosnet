<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="cosnet.android">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".CosplayItemsList">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".ShowCosplay" />
    </activity>
    <activity android:name=".ui.cosplay.EditCosplay" android:parentActivityName=".ui.cosplay.ShowCosplay"></activity>
    <activity android:name=".ui.cosplay.ShowCosplay"  android:parentActivityName=".MainActivity"/>
    <activity android:name=".ui.cosplayItem.AddCosplayItem" ><meta-data
      android:name="android.support.PARENT_ACTIVITY"
      android:value=".ui.cosplay.ShowCosplay" /></activity>
    <activity
      android:name=".MainActivity"
      android:label="@string/app_name"
      android:theme="@style/AppTheme.NoActionBar">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ui.cosplay.AddCosplay" android:parentActivityName=".MainActivity"/>

    <meta-data
      android:name="preloaded_fonts"
      android:resource="@array/preloaded_fonts" />
  </application>

</manifest>
